{
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "lib": [
      "es2017",
      "dom"
    ],
    "moduleResolution": "node",
    "newLine": "lf",
    "noEmitHelpers": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "strict": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "paths": {
      "@bellerophon-mobile/hippo": [
        "dist/hippo"
      ],
      "@bellerophon-mobile/hippo/webcrypto": [
        "dist/hippo/webcrypto"
      ],
      "@bellerophon-mobile/angular-hippo": [
        "dist/angular"
      ]
    }
  }
}
